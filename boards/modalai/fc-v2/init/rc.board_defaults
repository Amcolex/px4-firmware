#!/bin/sh
#
# board specific defaults
#------------------------------------------------------------------------------

# enable OSD on FCv2-J1 TELEM1 port by default (/dev/ttyS6)
param set-default MSP_OSD_CONFIG 101
param set-default SER_TEL1_BAUD 921600

# Disable TELEM2, used by ModalAI ESC (/dev/ttyS4)
param set-default MAV_1_CONFIG 0

# use ModalAI ESC by default
param set-default UART_ESC_CONFIG 1
